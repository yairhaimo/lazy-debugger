{
  "name": "lazy-debugger",
  "displayName": "Lazy Debugger",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.lazydebugger"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.lazydebugger",
        "title": "Toggle Lazy Debugger"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.1",
    "@types/babel__template": "^7.0.2",
    "@types/babel__traverse": "^7.0.12",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@babel/generator": "^7.10.2",
    "@babel/parser": "^7.10.2",
    "@babel/template": "^7.10.1",
    "@babel/traverse": "^7.10.1",
    "@babel/types": "^7.10.2"
  }
}
